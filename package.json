{
  "name": "desafio-cypress",
  "version": "1.0.0",
  "description": "Testes automatizados para fluxo de e-commerce na Amazon",
  "main": "index.js",
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test": "cypress run --spec 'cypress/e2e/**/*.cy.js'",
    "test:headed": "cypress run --headed --spec 'cypress/e2e/**/*.cy.js'",
    "test:parallel": "cypress run --parallel --record --key YOUR_RECORD_KEY",
    "test:fast": "cypress run --spec 'cypress/e2e/**/*.cy.js' --browser chrome --headless",
    "test:ci": "cypress run --spec 'cypress/e2e/**/*.cy.js' --browser chrome --headless --config-file cypress.config.ci.js",
    "test:split": "cypress run --spec 'cypress/e2e/**/*.cy.js' --browser chrome --headless --parallel --record --key YOUR_RECORD_KEY",
    "test:performance": "cypress run --config-file cypress.config.ci.js --spec 'cypress/e2e/**/*.cy.js'",
    "test:no-cache": "cypress run --spec 'cypress/e2e/**/*.cy.js' --browser chrome --headless --config-file cypress.config.ci.js",
    "test:benchmark": "npm run test:performance && npm run test:no-cache",
  "report:generate": "echo 'Allure report generation script aqui'",
    "report:clean": "rimraf cypress/reports",
    "test:with-report": "npm run test && npm run report:generate",
    "test:performance:with-report": "npm run test:performance && npm run report:generate",
    "report:combine": "node scripts/combine-reports.js",
    "cache:clear": "cypress cache clear",
    "cache:list": "cypress cache list",
    "cache:prune": "cypress cache prune"
  },
  "keywords": [
    "cypress",
    "e2e",
    "testing",
    "amazon",
    "ecommerce"
  ],
  "author": "Desafio Cypress",
  "license": "MIT",
  "devDependencies": {
    "cypress": "^13.17.0",
    "cypress-multi-reporters": "^2.0.5",

    "rimraf": "^6.0.1"
  }
}
